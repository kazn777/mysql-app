<div class = "index-list">
    <h1>ユーザー検索</h1>
    <%= search_form_for @q do |f| %>
  <%= f.label :age %>
  <%= f.number_field :age_gteq %>以上〜
  <%= f.number_field :age_lt %>未満<br />

  <%= f.label :prefecture_id, "都道府県" %>
  <%= f.collection_select :prefecture_id_eq, Prefecture.all, :id, :name, include_blank: '選択して下さい' %>

  <%= f.radio_button :sex_not_eq, selected: current_user.sex  %>

    <%= f.submit "検索" %>
    <% end %>
  </div>
</div>